                    android:layout_height="wrap_content"
                    android:text=""
                    android:fontFamily="sans-serif"
                    android:textStyle="bold"
                    android:textSize="15dp"
                    android:textColor="#333"/>

                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
